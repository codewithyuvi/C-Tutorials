#include <iostream>
using namespace std;

int main()
{
    int n;
    cout<<"enter";
    cin>>n;
    
    int i=1;
    int value='A';
    
    while(i<=n){
        int j=1;
        while(j<=n){
            char final_value = char(value);
            cout << final_value;
            
            j=j+1;
            
            
        }
        value=value+1;
        cout<<endl;
    i=i+1;
        
        
        
    }
    
    
    
    
    
    
    
    
}


