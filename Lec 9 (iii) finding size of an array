#include <iostream>
using namespace std ;

//HOLD
void printArray(int arr[], int size ){
    
    
    //print the array
    cout << "Printing tje array " << endl ; 
    for(int i = 0 ; i < size ; i++){
        cout << arr[i] << " ";    
    }
    
    cout << "Printing done" ;
    
}

int main()
{
    int third[15] = {2,7} ; 
    int n = 15;
    
     int thirdsize = sizeof(third)/sizeof(int) ; 
    cout <<endl << "size of third is " << thirdsize << endl ;
    
    
    cout << endl ; 
    
    int fifth[10] = {1};    
    
    //code to find size of an array 
    
    int fifthsize = sizeof(fifth)/sizeof(int) ; 
    cout <<endl << "size of fifth is " << fifthsize << endl ;
    
    
    
}
