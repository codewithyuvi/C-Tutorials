#include <iostream>
using namespace std;

int sum = 0 ;
int arrSum( int arr[], int size){
    
    for(int i =0; i<size; i++){
        sum = sum + arr[i];
    }
    return sum; 
}

int main()
{
    
    int size ;
    cin >> size ; 
    
    int arr[1000] ;
    for(int i = 0; i<size; i++){
        cin >> arr[i]; 
    }
    
    arrSum(arr, size) ;
    cout << sum ;

}
