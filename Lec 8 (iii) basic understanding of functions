#include <iostream>
using namespace std;

// int power(int a, int b){
    
// }
int power(){
    int a, b;
    cin >> a >> b;
    
    
    int ans =1;
    
    for (int i=1; i<=b; i++){
        ans = ans *a;
    }
    return ans;
    
}

