#include <iostream>
using namespace std; 

// 1 -> prime np.
// 0 -> not a prime no.

bool isPrime( int n){
    
    int remainder ; 
    for( int i = 2 ; i<n ; i++ ){
        remainder = n % i ;
        
        if(remainder == 0 ){
            return 0 ;
        }
    }
    
return 1 ;
    
    
}


int main()
{
    int n ;
    cin >> n ;
    
    if (isPrime(n) == 1 ) {
        cout << "Prime Number" ;
    }
    
    else{
        cout << "Not a Prime Number" ;
    }
    
    
}
