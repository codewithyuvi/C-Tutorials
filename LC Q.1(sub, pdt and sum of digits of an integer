#include <iostream>
using namespace std;


int main(){
    
    int n;
    cin>>n;
    
    int pdt=1;
    int sum=0;
    
    while(n!=0){
        int digit = n%10;
        pdt = pdt * digit;
        sum = sum + digit;
        
        n=n/10;
        
    }
    int answer = pdt - sum;
    cout<<answer;
    








    
    
}
