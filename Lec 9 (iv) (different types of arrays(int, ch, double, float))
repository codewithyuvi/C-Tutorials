#include <iostream>
using namespace std ;




int main()
{
    char ch[5] = {'a', 'b', 'c', 'r', 'p'};
    
    cout << ch[3] << endl ; 
    
    cout << "printing the array" << endl ; 
    
    for(int i = 0; i < 5 ; i++ ){
        cout << ch[i] << " " ; 
        
    } 
    
    cout <<endl << "Printing done :-)" ;
    
    double firstDouble[5];
    float firstFloat[6] ;
    bool firstBool[9] ;
}
