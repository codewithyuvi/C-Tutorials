#include <iostream>
using namespace std;

int main()
{
    int n;
    cout<<"enter";
    cin>>n;
    
    int row=1;
    
    
    while(row<=n){
        int col=1;
        
        char starting_ch = 'A' + n - row;
        while(col<=row){
    
            cout<< starting_ch  ;
            starting_ch = starting_ch +1;
            
            
            
            col=col+1;
            
        }
        cout<<endl;
        row=row+1;
        
    }
}


