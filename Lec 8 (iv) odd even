#include <iostream>
using namespace std;


/*
    1 in case of even
   0 in case of odd 
*/

bool isEven(int a){
    //odd
    if(a&1){
        return 0;
    }
    
    //even
    return 1;
    
}


int main()
{
    
    /* even odd*/
    
    int num;
    cin>> num;
    
    if(isEven(num)){
        cout<< "Number is Even" << endl;
    }
    else{
        cout << "Number is Odd"  << endl;
    }   
}
