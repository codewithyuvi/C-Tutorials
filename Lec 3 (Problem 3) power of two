#include <iostream>
#include <math.h>
using namespace std;

int main(){


    int n;
    cin>>n ;

    for( int i = 0; i <= 30; i++){
        int ans = pow(2, i);

        if(ans == n){
            cout << "true" ;
        }
    }
    cout << "false" ;
}


/* code for leetcode 
class Solution {
public:
    bool isPowerOfTwo(int n) {
        for( int i = 0; i <= 30; i++){
        int ans = pow(2, i);

        if(ans == n){
            return true ;
        }
    }
    return false ;

    }
};
